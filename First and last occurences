#include <iostream>
#include<bits/stdc++.h>
using namespace std;
vector<int>v={2,4,10,10,10,10,12,20};
int firstoccurence(vector<int>v)
{
    int mid,i=0,j=v.size(),tar=10,res=-1,f,l;
     while (i<=j)
    {
        mid=(i+j)/2;
        if (v[mid]==tar)
        {
         res=mid;
         j=mid-1;
        }
        else if (v[mid]<tar)
        i=mid+1;
        
        else
        j=mid-1;
    }

return res;
  
    
}
int lastocurrence(vector<int>v)
{
    int mid,i=0,j=v.size(),tar=10,res=-1,f,l;
     while (i<=j)
    {
        mid=(i+j)/2;
        if (v[mid]==tar)
        {
           
         res=mid;
         i=mid+1;
        }
        else if (v[mid]<tar)
        i=mid+1;
        
        else
        j=mid-1;
}

return res;

    
}


int main()
{
    int f,l;
    
    
f=firstoccurence(v);

cout<<"first occurencce "<<f <<endl;

l=lastocurrence(v);

cout<<"last occurencce "<<l;

    return 0;
}
